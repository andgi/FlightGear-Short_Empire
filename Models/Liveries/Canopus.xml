<PropertyList>
 <sim>
  <model>
   <livery>
    <name type="string">Canopus</name>
    <index type="int">0</index>
    <texture>Liveries/Canopus-fuselage.png</texture>
   </livery>
   <mark type="int">1</mark>
  </model>
 </sim>
 <fdm>
  <jsbsim>
   <fcs>
    <fuel-system>
     <mark type="double">1.0</mark>
    </fuel-system>
   </fcs>
  </jsbsim>
 </fdm>
 <consumables>
   <fuel>
     <!-- Setting a capacity of zero in a tank hides it from the Fuel and Payload dialog. -->
     <tank n="2"><capacity-gal_us type="double">0</capacity-gal_us></tank>
     <tank n="3"><capacity-gal_us type="double">0</capacity-gal_us></tank>
     <tank n="4"><capacity-gal_us type="double">0</capacity-gal_us></tank>
     <tank n="5"><capacity-gal_us type="double">0</capacity-gal_us></tank>
     <tank n="6"><capacity-gal_us type="double">0</capacity-gal_us></tank>
     <tank n="7"><capacity-gal_us type="double">0</capacity-gal_us></tank>
   </fuel>
 </consumables>
 <limits>
  <mass-and-balance>
   <maximum-takeoff-mass-lbs type="double">40500</maximum-takeoff-mass-lbs>
   <maximum-landing-mass-lbs type="double">40500</maximum-landing-mass-lbs>
  </mass-and-balance>
 </limits>
</PropertyList>
