 <!-- Particle effects -->
 <model>
  <name>Step 1</name>
  <overlay>
   <particlesystem>
    <condition>
     <and>
      <greater-than>
       <property>fdm/jsbsim/hydro/coefficients/C_Delta</property>
       <value>0.0</value>
      </greater-than>
      <greater-than>
       <property>velocities/groundspeed-kt</property>
       <value>3.0</value>
      </greater-than>
     </and>
    </condition>
    <shooter>
     <phi-min-deg>90.0</phi-min-deg>
     <phi-max-deg>90.0</phi-max-deg>
    </shooter>
    <particle>
     <end>
      <size>
       <value>0.50</value>
       <spread>0.2</spread>
      </size>
     </end>
     <life-sec>
      <value>10</value>
     </life-sec>
    </particle>
   </particlesystem>
  </overlay>
  <path>Aircraft/Short_Empire/Models/Effects/wake.xml</path>
  <offsets>
   <heading-deg>180</heading-deg>
   <!-- x/y/z == forward/left/up -->
   <x-m>  -9.80 </x-m>
   <y-m>   0.00 </y-m>
   <z-m>  -1.84 </z-m>
  </offsets>
 </model>
 <model>
  <name>Step 2</name>
  <overlay>
   <particlesystem>
    <condition>
     <and>
      <greater-than>
       <property>fdm/jsbsim/hydro/coefficients/C_Delta</property>
       <value>0.0</value>
      </greater-than>
      <greater-than>
       <property>velocities/groundspeed-kt</property>
       <value>3.0</value>
      </greater-than>
     </and>
    </condition>
    <shooter>
     <phi-min-deg>-90.0</phi-min-deg>
     <phi-max-deg>-90.0</phi-max-deg>
    </shooter>
    <particle>
     <end>
      <size>
       <value>0.50</value>
       <spread>0.2</spread>
      </size>
     </end>
     <life-sec>
      <value>10</value>
     </life-sec>
    </particle>
   </particlesystem>
  </overlay>
  <path>Aircraft/Short_Empire/Models/Effects/wake.xml</path>
  <offsets>
   <heading-deg>180</heading-deg>
   <!-- x/y/z == forward/left/up -->
   <x-m>  -9.80 </x-m>
   <y-m>   0.00 </y-m>
   <z-m>  -1.84 </z-m>
  </offsets>
 </model>

 <animation>
  <type>translate</type>
  <object-name>Step 1</object-name>
  <object-name>Step 2</object-name>
  <property>fdm/jsbsim/hydro/height-agl-ft</property>
  <factor>-0.3048</factor>
  <offset-m>2.10</offset-m>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>

 <model>
  <name>Left float 1</name>
  <overlay>
   <particlesystem>
    <condition>
     <and>
      <greater-than>
       <property>fdm/jsbsim/hydro/left-float-submersion-ft</property>
       <value>0.0</value>
      </greater-than>
      <greater-than>
       <property>velocities/groundspeed-kt</property>
       <value>3.0</value>
      </greater-than>
     </and>
    </condition>
    <shooter>
     <phi-min-deg>90.0</phi-min-deg>
     <phi-max-deg>90.0</phi-max-deg>
    </shooter>
   </particlesystem>
  </overlay>
  <path>Aircraft/Short_Empire/Models/Effects/wake.xml</path>
  <offsets>
